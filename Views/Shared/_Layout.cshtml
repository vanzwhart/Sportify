
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Reporting</title>
    <!-- Tell the browser to be responsive to screen width -->
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="~/Contentpro/css/pages/footable-page.css" rel="stylesheet">
    <link href="~/Contentpro/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Contentpro/plugins/perfect-scrollbar/dist/css/perfect-scrollbar.min.css" rel="stylesheet">
    <link href="~/Contentpro/plugins/vectormap/jquery-jvectormap-2.0.2.css" rel="stylesheet" />
    <link href="~/Contentpro/plugins/chartist-js/dist/chartist.min.css" rel="stylesheet">
    <link href="~/Contentpro/plugins/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.css" rel="stylesheet">
    <link href="~/Contentpro/css/colors/default.css" id="theme" rel="stylesheet">
    <link href="~/Contentpro/css/pages/dashboard4.css" rel="stylesheet">
    <link href="~/Contentpro/css/style.css" rel="stylesheet">
    <link href="../assets/plugins/Magnific-Popup-master/dist/magnific-popup.css" rel="stylesheet">
    <link href="~/Contentpro/css/pages/user-card.css" rel="stylesheet">
   
    <link href="~/Contentpro/plugins/toast-master/css/jquery.toast.css" rel="stylesheet">
    <link href="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css" rel="stylesheet">
    <link href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.bootstrap4.min.css" rel="stylesheet" />
    <!-- Favicon icon -->

</head>

<body class="fix-header fix-sidebar card-no-border">
    <div class="preloader">
        <div class="loader">
            <div class="loader__figure"></div>
            <p class="loader__label">Online Expert</p>
        </div>
    </div>
    <div id="main-wrapper">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        @using Microsoft.AspNet.Identity

        <header class="topbar">
            <nav class="navbar top-navbar navbar-expand-md navbar-light">
                <!-- ============================================================== -->
                <!-- Logo -->
                <!-- ============================================================== -->
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">
                        <!-- Logo icon --><b>
                            <!--You can put here icon as well // <i class="wi wi-sunset"></i> //-->
                            <!-- Dark Logo icon -->
                            <img src="~/Contentpro/images/sportify.png" alt="homepage" class="dark-logo" />
                            <!-- Light Logo icon -->
                            <img src="~/Contentpro/images/sportify.png" alt="homepage" class="light-logo" />
                        </b>
                        <!--End Logo icon -->
                        @*<!-- Logo text --><span>
                                <!-- dark Logo text -->
                                <img src="../assets/images/logo-text.png" alt="homepage" class="dark-logo" />
                                <!-- Light Logo text -->
                                <img src="../assets/images/logo-light-text.png" class="light-logo" alt="homepage" />
                            </span>*@
                    </a>
                </div>
                <!-- ============================================================== -->
                <!-- End Logo -->
                <!-- ============================================================== -->
                <div class="navbar-collapse">
                    <!-- ============================================================== -->
                    <!-- toggle and nav items -->
                    <!-- ============================================================== -->
                    <ul class="navbar-nav mr-auto">
                        <!-- This is  -->
                        <li class="nav-item"> <a class="nav-link nav-toggler hidden-md-up waves-effect waves-dark" href="javascript:void(0)"><i class="ti-menu"></i></a> </li>
                        <li class="nav-item"> <a class="nav-link sidebartoggler hidden-sm-down waves-effect waves-dark" href="javascript:void(0)"><i class="ti-menu"></i></a> </li>
                    </ul>
                    <!-- ============================================================== -->
                    <!-- User profile and search -->
                    <!-- ============================================================== -->

                    <a>Hi, </a>
                    @if (User.Identity.IsAuthenticated)
                    {
                        using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                        {
                            @Html.AntiForgeryToken()
                            <ul class="navbar-nav my-lg-0">

                                <!-- Profile -->
                                <!-- ============================================================== -->
                                <li class="nav-item dropdown">
                                    <a class="nav-link dropdown-toggle waves-effect waves-dark" href="#" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><img src="" alt="user" class="profile-pic" /></a>
                                    <div class="dropdown-menu dropdown-menu-right animated flipInY">
                                        <ul class="dropdown-user">
                                            <li>
                                                <div class="dw-user-box">
                                                    <div class="u-img"><img src="" alt="user"></div>
                                                    <div class="u-text">
                                                        <h4></h4>

                                                    </div>
                                                </div>
                                            </li>
                                            <li role="separator" class="divider"></li>
                                            <li><a href="#"><i class="ti-user"></i> Change Account</a></li>
                                            <li role="separator" class="divider"></li>
                                            <li><a href="#"><i class="ti-settings"></i> Account Setting</a></li>
                                            <li role="separator" class="divider"></li>
                                            <li><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>

                                        </ul>
                                    </div>
                                </li>
                            </ul>
                        }
                    }

                </div>
            </nav>
        </header>
        <!-- ============================================================== -->
        <!-- End Topbar header -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <aside class="left-sidebar">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">


                        <li>
                            <a class="has-arrow waves-effect waves-dark" href="#" aria-expanded="false"><i class="fas fa-address-book"></i><span class="hide-menu">MyExercises</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="/Account/EditUserInfo">Edit Exercises </a></li>
                                <li><a href="/Home/Index">Exercises</a></li>
                            </ul>
                        </li>
                        <li>
                            <a class="has-arrow waves-effect waves-dark" href="#" aria-expanded="false"><i class="fas fa-clipboard-list"></i><span class="hide-menu"> Movement</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="/Movement/Index">All Movements</a></li>
                               
                            </ul>
                        </li>
                        <li>
                            <a class="has-arrow waves-effect waves-dark" href="#" aria-expanded="false"><i class="fas fa-chart-bar"></i><span class="hide-menu"> Foods</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="/Food/Index">All Foods</a></li>
                                <li><a href="/Food/MyFood">My Foods</a></li>

                            </ul>
                        </li>
                        @if(User.IsInRole("Admin"))
                        {
                        <li>
                            <a class="has-arrow waves-effect waves-dark" href="#" aria-expanded="false"><i class="fas fa-registered"></i><span class="hide-menu"> Register</span></a>
                            <ul aria-expanded="false" class="collapse">
                                <li><a href="/Account/RegisterAdmin">Register Admin</a></li>
                              
                            </ul>
                        </li>
                        }

                    </ul>
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->

        <div class="page-wrapper">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
    </div>

    <script src="~/Contentpro/plugins/dff/dff.js"></script>

    <script src="~/Contentpro/plugins/toast-master/js/jquery.toast.js"></script>

    <!-- Bootstrap popper Core JavaScript -->
    <script src="~/Contentpro/plugins/bootstrap/js/popper.min.js"></script>
    <script src="~/Contentpro/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="~/Contentpro/js/perfect-scrollbar.jquery.min.js"></script>
    <!--Wave Effects -->
    <script src="~/Contentpro/js/waves.js"></script>
    <!--Menu sidebar -->
    <script src="~/Contentpro/js/sidebarmenu.js"></script>
    <!--Custom JavaScript -->
    <script src="~/Contentpro/js/custom.min.js"></script>
    <script src="~/Contentpro/plugins/Magnific-Popup-master/dist/jquery.magnific-popup.min.js"></script>
    <script src="~/Contentpro/plugins/Magnific-Popup-master/dist/jquery.magnific-popup-init.js"></script>
    <!-- ============================================================== -->
    <!-- This page plugins -->
    <!-- ============================================================== -->
    <!-- Vector map JavaScript -->
    <script src="~/Contentpro/plugins/vectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/Contentpro/plugins/vectormap/jquery-jvectormap-world-mill-en.js"></script>
    <!--sparkline JavaScript -->
    <script src="~/Contentpro/plugins/sparkline/jquery.sparkline.min.js"></script>
    <!--morris JavaScript -->
    <script src="~/Contentpro/plugins/chartist-js/dist/chartist.min.js"></script>
    <script src="~/Contentpro/plugins/chartist-plugin-tooltip-master/dist/chartist-plugin-tooltip.min.js"></script>
    <!-- Chart JS -->
    <script src="~/Scripts/bootbox.js"></script>
    <script src="~/Contentpro/js/dashboard4.js"></script>
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/DataTables/dataTables.bootstrap.js"></script>
    <script src="~/Scripts/DataTables/dataTables.responsive.min.js"></script>
    <!-- ============================================================== -->
    <!-- Style switcher -->
    <!-- ============================================================== -->
    <script src="~/Contentpro/plugins/styleswitcher/jQuery.style.switcher.js"></script>

    <script src="~/Contentpro/plugins/toast-master/js/jquery.toast.js"></script>
    @Scripts.Render("~/bundles/lib")
    @RenderSection("scripts", required: false)

</body>
</html>

